<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="394.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ModifyController">
   <children>
      <TextField fx:id="idTextField" layoutX="113.0" layoutY="46.0" promptText="ID" />
      <TextField fx:id="descriereTextField" layoutX="113.0" layoutY="105.0" promptText="descriere" />
      <TextField fx:id="startWeekTextField" layoutX="113.0" layoutY="172.0" promptText="start week" />
      <TextField fx:id="deadlineWeekTextField" layoutX="113.0" layoutY="238.0" promptText="deadline week" />
      <Button fx:id="modifyBtn" layoutX="412.0" layoutY="80.0" mnemonicParsing="false" onAction="#handleModify" text="SAVE" />
      <Button fx:id="clearBtn" layoutX="411.0" layoutY="147.0" mnemonicParsing="false" onAction="#handleClear" text="CLEAR" />
      <Button fx:id="cancelBtn" layoutX="412.0" layoutY="213.0" mnemonicParsing="false" onAction="#cancel" text="CANCEL" />
   </children>
</AnchorPane>
